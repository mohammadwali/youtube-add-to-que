{

  "manifest_version": 2,
  "name": "Youtube playlist",
  "short_name": "Youtube Que",
  "description": "Can add order or modify current video que",
  "version": "1.0.0",
  "icons": {
    // "16": "images/icon16.png",
    // "48": "images/icon48.png",
    // "128": "images/icon128.png"
  },
  "app": {
    "launch": {
      "local_path": "launchApp.html",
      "container": "panel",
      "width": 0,
      "height": 0
    }
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "unlimitedStorage",
    "contextMenus",
    "tabs",
    "http://www.youtube.com/*",
    "https://www.youtube.com/*",
    "http://gdata.youtube.com/*",
    "http://fonts.gstatic.com/*",
    "http://cdnjs.cloudflare.com/*"
  ],
  "background": {
    "scripts": [
      "js/localstorageHelper.js",
      "js/bg.js"
    ]
  },
  "content_scripts": [{
    "matches": [
      "http://www.youtube.com/*",
      "https://www.youtube.com/*"
    ],
    "js": ["js/jquery-1.11.1.min.js", "js/cs.js"]
  }],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com https://www.youtube.com https://s.ytimg.com https://fonts.gstatic.com https://cdnjs.cloudflare.com; object-src 'self' https://www.youtube.com",
  "options_page": "options.html",
  "minimum_chrome_version": "41",
  "optional_permissions": [],
  "web_accessible_resources": [
    "fonts/*.*",
    "css/*.css",
    "*"
  ]

}
